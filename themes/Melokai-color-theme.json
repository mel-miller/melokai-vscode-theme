        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#fc9867"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
